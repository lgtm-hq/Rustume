*.rmeta
*.d

# Dependencies
node_modules/

# Virtual environments
.venv/
venv/
env/

# Lock files
Cargo.lock
bun.lock
*.lock

# Git
.git/

# IDE
.idea/
.vscode/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Generated files
*.generated.*

# WASM bindings (generated)
bindings/wasm/pkg/

# Test fixtures and samples
test_samples/

# Coverage reports
htmlcov/
coverage/
